    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "run",
            ],
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch Syncer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "sync",
                "--level",
                "debug"
            ],
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch Validator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "validate",
                "--level",
                "debug",
                "--dns_service",
                "localhost:8081",
                "--force"
            ],
            "program": "${workspaceFolder}"
        }
    ]
}